{
  "npmScope": "react-quick-hacks",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "auth": {
      "tags": []
    },
    "firebase-auth": {
      "tags": []
    },
    "layout": {
      "tags": []
    },
    "mbob-scanner": {
      "tags": []
    },
    "mbob-scanner-e2e": {
      "tags": [],
      "implicitDependencies": ["mbob-scanner"]
    },
    "mbox": {
      "tags": []
    },
    "mbox-e2e": {
      "tags": [],
      "implicitDependencies": ["mbox"]
    },
    "shared": {
      "tags": []
    },
    "ui-kit": {
      "tags": []
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "strict": true,
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint",
        "strict": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "mbob-scanner"
}
